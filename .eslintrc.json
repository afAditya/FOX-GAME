{
  "extends": {
    "eslint:recommended",
    "plugin:import/errrors",
    "prettier"
  },
  "rules": {
    "no-console": 1
  },
  "plugins": ["imports"],
  "parserOptions": {
    "ecmaversion":2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
